<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->

<html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">

        {% block title %}
		{{ headTitle('Tuinplanten') }}
		{% endblock %}

        {% block link %}
		{% do headLink({'rel': 'shortcut icon', 'href': '/images/favicon.ico'}) %}
		{{ headLink() }}
		{% endblock %}

        <meta name="description" content="">
        <meta name="author" content="">
        <meta name="robots" content="noindex, nofollow">

        {% block meta %}
		{% do headMeta('viewport', 'width=device-width, initial-scale=1.0') %}
		{{ headMeta() }}
		{% endblock %}

        <!-- Icons -->
        <!-- The following icons can be replaced with your own, they are used by desktop and mobile browsers -->
        <link rel="shortcut icon" href="img/favicon.ico">
        <link rel="apple-touch-icon" href="img/apple-touch-icon.png">
        <link rel="apple-touch-icon" sizes="57x57" href="img/apple-touch-icon-57x57-precomposed.png">
        <link rel="apple-touch-icon" sizes="72x72" href="img/apple-touch-icon-72x72-precomposed.png">
        <link rel="apple-touch-icon" sizes="114x114" href="img/apple-touch-icon-114x114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="img/apple-touch-icon-precomposed.png">
        <!-- END Icons -->

        <!-- Stylesheets -->
        <!-- The roboto font is included from Google Web Fonts -->
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:400,400italic,700,700italic">

        <base href="/" />
        <link rel="stylesheet" href="../../../css/main.css" />
        <link rel="stylesheet" href="../../../css/bootstrap.css" />
        <link rel="stylesheet" href="../../../css/plugins.css" />
        <link rel="stylesheet" href="../../../css/tuinplanten.css" />

        <link rel="stylesheet" href="../../../js/jqueryUI/themes/base/jquery-ui.css" />
        <link rel="stylesheet" href="../themes/tuinplanten/style.css" type="text/css" media="print, projection, screen" />

        <!-- END Stylesheets -->

        <!-- Modernizr (Browser feature detection library) & Respond.js (Enable responsive CSS code on browsers that don't support them) -->
        <script src="../../../js/vendor/modernizr-2.6.2-respond-1.1.0.min.js"></script>
    </head>

    <body class="fixed980">

        <!-- Page Container -->
        <div id="page-container">

            <header class="">
                <div id="logo"></div>

                <h1>Tuinplanten</h1>
            </header>
            <!-- END Header -->
                              {% if flashMessenger().hasMessages() %}
            <div class="alert">
            {% for message in flashMessenger().getMessages() %}
                {{ message }}
            {% endfor %}
                </div>
        {% endif %}

                <div id="container">
                    <div id="site">
                        <div id="content_container">

                            <div id="inner-container">
                                <!-- Sidebar -->
                                <aside id="page-sidebar" class="nav-collapse">
                                    <!-- Sidebar search -->
                                    <form id="sidebar-search" method="post" onsubmit="return false;">
                                        <div class="input-append">
                                            <input type="text" placeholder="Search.." class="remove-box-shadow remove-transition remove-radius">
                                            <button><i class="icon-search"></i></button>
                                        </div>
                                    </form>
                                    <!-- END Sidebar search -->

                                    <!-- Primary Navigation -->
                                    <nav id="primary-nav">
                {% include 'garden/template/display_nav.twig' %}
                                        </nav>
                                        <!-- END Primary Navigation -->

                                    </aside>
                                    <!-- END Sidebar -->
                                    <!-- Page Content -->
                                    <div id="page-content">
                                        <!-- Navigation info -->
                                        <ul id="nav-info" class="clearfix">
                                            <li><a href="/"><i class="icon-home"></i></a></li>
                                        </ul>
                                        <!-- END Navigation info -->
                                            {{ content | raw }}
                                    </div>

                                </div>


                            </div>
                            <div class="clear"></div>
                            <div id="footer">
                            </div>
                        </div>
                    </div>

                    <h2>Footer</h2>
                    <!-- Excanvas for Flot (Charts plugin) support on IE8 -->
                    <!--[if lte IE 8]><script src="js/helpers/excanvas.min.js"></script><![endif]-->

                    <!-- Jquery library from Google ... -->

                    {% do inlineScript().prependFile('/js/bootstrap.min.js') %}
                        {% do inlineScript().prependFile('http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js') %}
                        {% do inlineScript().prependFile('http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js') %}
                        {% do inlineScript().appendFile('/js/jquery.tablesorter.min.js') %}
                        {% do inlineScript().appendFile('/addons/pager/jquery.tablesorter.pager.js') %}
                        {% do inlineScript().appendFile('/js/tuinplanten.js') %}
                        {% do inlineScript().appendFile('/js/plugins.js') %}
                        {% do inlineScript().appendFile('/js/main.js') %}
                    {{ inlineScript() }}


                    <!-- Include Google Maps API for global use -->
                    <!-- Jquery plugins and custom javascript code
                    <script src="/js/plugins.js"></script>
                    <script src="/js/main.js"></script>
                    <script src="/js/jquery.tablesorter.min.js"></script>
                    <script src="/js/tuinplanten.js"></script>-->
            </body>
        </html>